    { "agent_type": "FeatureEngineerAgent", "params": {"strategy": "basic"} },
    { "agent_type": "ModelOptimizeAgent", "params": {"model_type": "random_forest", "max_iter": 10} },
    { "agent_type": "ReportWriterAgent", "params": {"format": "pdf"} }
  ]
